{
  "name": "prisma",
  "$schema": "../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "prisma/src",
  "projectType": "library",
  "targets": {
    "prisma": {
      "command": "prisma",
      "options": {
        "cwd": "prisma/src/lib"
      }
    },
    "studio": {
      "command": "prisma studio",
      "options": {
        "cwd": "prisma/src/lib"
      }
    },
    "generate-types": {
      "command": "prisma generate",
      "options": {
        "cwd": "prisma/src/lib"
      }
    },

    "seed": {
      "command": "ts-node ./seed.ts",
      "options": {
        "cwd": "prisma/src/lib"
      }
    },

    "migrate-dev": {
      "command": "prisma migrate dev",
      "options": {
        "cwd": "prisma/src/lib"
      }
    },
    "migrate-deploy": {
      "command": "prisma migrate deploy",
      "options": {
        "cwd": "prisma/src/lib"
      }
    }
  },
  "tags": []
}
